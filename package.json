{
  "name": "app",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.27.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/tailwind.css",
    "watch:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/tailwind.css --watch",
    "build": "esbuild app/javascript/application.js --bundle --format=esm --outdir=app/assets/builds --public-path=/assets --loader:.svg=file --loader:.png=file --loader:.woff=file --loader:.woff2=file --sourcemap=external",
    "watch": "esbuild app/javascript/application.js --bundle --format=esm --outdir=app/assets/builds --public-path=/assets --loader:.svg=file --loader:.png=file --loader:.woff=file --loader:.woff2=file --sourcemap=external --watch"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "@tailwindcss/cli": "^4.1.18",
    "abcjs": "^6.5.2",
    "ace-builds": "^1.43.5",
    "alpinejs": "^3.15.3",
    "effector": "^23.4.4",
    "lodash": "^4.17.21"
  }
}
